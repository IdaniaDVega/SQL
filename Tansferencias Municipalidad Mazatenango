### ANALYSIS

-- Sum of total transfer amounts by criteria, and filtered by years 2024/2025

SELECT codigo_criterio, SUM(monto) AS total_montos 
FROM subsidios 
WHERE fecha LIKE '%2025' 
GROUP BY codigo_criterio;

SELECT codigo_criterio, SUM(monto) AS total_montos 
FROM subsidios 
WHERE fecha LIKE '%2024' 
GROUP BY codigo_criterio;

-- Amounts of money transferred by the Municipality filtered by year 2024/2025 and categorized accordingly.

SELECT beneficiario, criterio, monto, 
CASE WHEN monto >= 8000 THEN 'Gasto Alto'
WHEN monto >= 5000 THEN 'Gasto Moderado'
WHEN monto >= 1500 THEN 'Gasto Regular'
ELSE 'Gasto Bajo'
END AS Tipo_de_Gasto
FROM subsidios
WHERE fecha LIKE '%2025'
ORDER BY monto asc;

-- Sum of total amounts transferred by the Municipality by beneficiary and ordered by the total amounts so it's easier to spot the top beneficiary. Also filtered by year.

SELECT beneficiario, SUM(monto) AS monto_total_2024 
FROM subsidios 
WHERE fecha LIKE '%2024' 
GROUP BY beneficiario 
ORDER BY monto_total_2024;

SELECT beneficiario, SUM(monto) AS monto_total_2025 
FROM subsidios 
WHERE fecha LIKE '%2025' 
GROUP BY beneficiario 
ORDER BY monto_total_2025;

-- Created two views which would make it easier to define the money transferred to "Asociacion Deportivo CSD Suchitepequez" which is the local soccer team.

Create view CSD_2025 AS Select beneficiario, codigo_criterio, criterio, monto, fecha 
FROM subsidios 
WHERE fecha LIKE '%2025' AND beneficiario LIKE '%CSD%';

Create view CSD_2024 AS Select beneficiario, codigo_criterio, criterio, monto, fecha 
FROM subsidios 
WHERE fecha LIKE '%2024' AND beneficiario LIKE '%CSD%';

-- Query to find all transfers made to the female soccer team.

SELECT beneficiario, monto, fecha 
FROM subsidios 
WHERE beneficiario LIKE '%femenina%';

-- Total amount transferred by beneficiary, this helps notice if there are duplicate beneficiaries.

SELECT beneficiario, SUM(monto) AS total_montos from subsidios WHERE fecha LIKE '%2025' group by beneficiario order by beneficiario asc;
SELECT beneficiario, SUM(monto) AS total_montos from subsidios WHERE fecha LIKE '%2024' group by beneficiario order by beneficiario asc;






